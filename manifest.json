{
    "manifest_version": 2,
    "name": "YouTube resumer",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/*",
                "*://*.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}